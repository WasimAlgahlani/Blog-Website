<%- include("partials/header"); -%>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
  <label>Title</label>
<input type="text" name="postTitle" class="form-control" autocomplete="off">
</div>
<div class="form-group">
<label>Post</label>
<textarea name="postBody" rows="6" cols="30" class="form-control"></textarea>
</div>
<button type="submit" name="subButton" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer"); -%>
